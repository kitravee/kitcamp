<%- include("../partials/header") %>

<div class="container">
    <div class="row justify-content-center">
        <h1>Notifications</h1>

        <ul>
            <% allNotifications.forEach(function(notification) { %>
            <li>
                <a href="/campgrounds/<%= notification.campgroundId %>">
                    <%= notification.username %> created a new campground %>
                </a>
            </li>
            <% }); %>
        </ul>
    </div>
</div>


<%- include("../partials/footer") %>